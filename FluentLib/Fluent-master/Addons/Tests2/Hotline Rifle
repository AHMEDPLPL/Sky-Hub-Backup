			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 - 0.05 * COS(SINE / 12), -0.01) * ANGLES(RAD(MRANDOM(1,360)), RAD(MRANDOM(1,360)), RAD(MRANDOM(1,360)), RAD(MRANDOM(1,360)), RAD(MRANDOM(1,360)), RAD(MRANDOM(1,360))), 0.15 / Animation_Speed)
