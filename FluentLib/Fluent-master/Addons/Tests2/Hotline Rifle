Player.Chatted:connect(function(text)
	if(text == "/e filter")then
		Filter = not Filter
	else
		if(Filter)then
			local succ,text = pcall(function() return game:service'Chat':FilterStringForBroadcast(text,Plr) end)
			if(not succ)then
				text = string.rep("_",#text)
			end
			Chat(text)
		else
			Chat(text)
		end
	end
end)
