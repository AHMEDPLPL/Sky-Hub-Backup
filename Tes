Tab = Window:AddTab({ Title = "FE Animations Hats", Icon = "rbxassetid://7743875962" })

local Section = Tab:AddSection("Main")

local genesis_supported = game:FindFirstChildOfClass("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid").BreakJointsOnDeath
if genesis_supported == true then
    genesis_supported = "Hat Reanimte will work on this game! :)"
else
    genesis_supported = "Hat Reanimte will sadly not work on this game :("
end

Tab:AddParagraph({
    Title = "Attention",
    Content = "These Scripts Need Hats Btw\nThese Animations Are Converted By Me"
})
local Global = getgenv and getgenv() or _G

if replicatesignal then

Tab:AddParagraph({
    Title = "Replicatesignal",
    Content = "Your executor supports replicatesignal, which allows Permadeath and Hat Reanimate to function in most games.\n\nNote: Permadeath may not be compatible with every game. If you notice it's not working, you can simply disable it."
})

Global.PremadeathEnabled = true

Tab:AddToggle("MyToggle", {Title = "Permanent Death", Default =Global.PremadeathEnabled  ,Callback = function(state)
Global.PremadeathEnabled = state
end})

else

Tab:AddParagraph({
    Title = "Replicatesignal",
    Content = "Your executor does not support replicatesignal, so Permadeath will not work.\n\ Executors that support replicatesignal:\n• Android: FrostWare, Nebula, Delta\n• iOS: Delta\n• Windows: Atlantis, Ronix, Seliware, Wave."
})

Tab:AddParagraph({
    Title = "Game Supported ?",
    Content = genesis_supported
})

end

Global.FlingEnabled = true

Tab:AddToggle("MyToggle", {Title = "Fling", Default =Global.FlingEnabled  ,Callback = function(state)
Global.FlingEnabled = state
end})


