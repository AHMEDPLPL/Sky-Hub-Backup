local HttpService = game:GetService("HttpService")
local player = game.Players.LocalPlayer
local placeName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local webhook = "https://discord.com/api/webhooks/1354646294257209364/rTJOV-Kz6Pxzw2glS5DJsdgAlw6Jy1ulAWRPerLyuT-lCDhauacn4ECejHVE01C9fWSG"

local feedbackTimes = {}

local function sendFeedback(txt)
    local currentTime = os.time()
    
    for i = #feedbackTimes, 1, -1 do
        if currentTime - feedbackTimes[i] > 3600 then
            table.remove(feedbackTimes, i)
        end
    end

    if #feedbackTimes >= 3 then
        OrionLib:MakeNotification({ 
            Name = "System Hub", 
            Content = "You can only send 3 feedbacks per hour!", 
            Image = "rbxassetid://6031075938", 
            Time = 5 
        })
        return
    end

    table.insert(feedbackTimes, currentTime)

    local data = {
        content = "",
        embeds = {
            {
                title = "Feedback From : " .. player.Name,
                description = "Executor : "..(identifyexecutor and identifyexecutor() or "Unknown").."   Game : "..placeName.."           Feedback : "..txt
            }
        }
    }
    local jsonData = HttpService:JSONEncode(data)
    local headers = { ["Content-Type"] = "application/json" }

    local request = syn and syn.request or http and http.request or http_request or request or fluxus and fluxus.request 
                    or oxygen and oxygen.http or comet and comet.request or yoink and yoink.request 
                    or venus and venus.request or WRD and WRD.request or solara and solara.request or HttpPost

if request then
    request({ Url = webhook, Method = "POST", Headers = headers, Body = jsonData })
        Notify("System Hub","Feedback sent successfully","",5)
    else
        Notify("System Hub","Failed to send feedback. Try another executor.","",5)
    end
end

local userInput = ""

local Input = Main:AddInput("Input", {
        Title = "Feedback",
        Default = "",
        Numeric = false,
        Finished = false, 
        Callback = function(Value)
            userInput = Value
        end
})

Main:AddButton({Title = "Send",Callback = function()
            if userInput ~= "" then
            sendFeedback(userInput)
        else
            Notify("System Hub","No Feedback to send","",5)
        end
end})

Main:AddButton({Title = "Discord Server",Callback = function()
        setclipboard("https://discord.gg/hh9gBDaWd4")
        Notify("System Hub","Discord Server Link copied to clipboard!","",5)
end})

