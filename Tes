local blacklistURL = "https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Scripts/Blacklist"

local success, blacklistData = pcall(function()
    return loadstring(game:HttpGet(blacklistURL))()
end)

if not success then
    blacklistData = {} 
end

if blacklistData[LocalPlayer.Name] then
    LocalPlayer:Kick("You are banned from this hub")
end

