Tab = Window:AddTab({ Title = "FE Scripts", Icon = "scroll" })

Tab:AddButton({Title = "R15 to R6 Client", Callback = function()
    loadstring(game:HttpGet("https://pastebin.com/raw/jHGVauVX", true))()
end})

Tab:AddButton({Title = "Reset", Callback = function()
        if FakeCharacter then
            FakeCharacter:Destroy()
        end
        local Player = game.Players.LocalPlayer
        local Character = Player.Character

        local function getRoot(char)
            return char:FindFirstChild("HumanoidRootPart") 
                or char:FindFirstChild("Torso") 
                or char:FindFirstChild("UpperTorso")
        end

        local function getChar()
            return Player.Character
        end

        local function respawn()
            local rootPart = getRoot(Character)
            if not rootPart then return end
            
            local cf = rootPart.CFrame
            Character:BreakJoints()
            
            Player.CharacterAdded:Wait()
            wait(0.2)

            local newChar = getChar()
            newChar:WaitForChild("HumanoidRootPart").CFrame = cf
        end

        local oldCFrame = getRoot(Character) and getRoot(Character).CFrame or nil
        if oldCFrame then
            respawn()
            Player.CharacterAdded:Wait()
            getChar():WaitForChild("HumanoidRootPart").CFrame = oldCFrame
        end
end})

if IsOnMobile then
    Tab:AddButton({Title = "Keyboard", Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Scripts/Keyboard"))() 
    end})
end

Tab:AddToggle("MyToggle", {Title = "Touch Fling", Default = false ,Callback = function(state)
        if state == true then
            walkflinging = false
            local RunService = game:GetService("RunService")
            local character = game.Players.LocalPlayer.Character
            
            function getRoot(char)
            local rootPart = game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart') or game.Players.LocalPlayer.Character:FindFirstChild('Torso') or game.Players.LocalPlayer.Character:FindFirstChild('UpperTorso')
      	  return rootPart
            end
            local humanoid = character and character:FindFirstChildWhichIsA("Humanoid")
            if humanoid then
                humanoid.Died:Connect(function()
                    walkflinging = false
                end)
            end

            walkflinging = true
            repeat
                RunService.Heartbeat:Wait()
                local root = getRoot(character)

                while not (character and character.Parent and root and root.Parent) do
                    RunService.Heartbeat:Wait()
                    character = speaker.Character
                    root = getRoot(character)
                end

                local vel = root.Velocity
                root.Velocity = vel * 10000 + Vector3.new(0, 10000, 0)

                RunService.RenderStepped:Wait()
                if character and character.Parent and root and root.Parent then
                    root.Velocity = vel
                end

                RunService.Stepped:Wait()
                if character and character.Parent and root and root.Parent then
                    root.Velocity = vel + Vector3.new(0, 0.1, 0)
                end
            until walkflinging == false
        else
            walkflinging = false
        end
    end
})

Tab:AddButton({Title = "Click Fling",  Callback = function()
      	loadstring(game:HttpGet("https://raw.githubusercontent.com/AHMEDPLPL/System-hub-V3/refs/heads/main/Scripts/Click%20fling"))()
end})

local Section = Tab:AddSection("FE Scripts converted by me")

Tab:AddButton({Title = "FE Immortality Lord",  Callback = function()
      	loadstring(game:HttpGet("https://gist.githubusercontent.com/AHMEDPLPL/98691a5a5e518e21d90919bfb8c13b65/raw/fe34c56992e7674520ea53c5fcbebce28429dbe8/FE%2520Immortality%2520Lord"))()
