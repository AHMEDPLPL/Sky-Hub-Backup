local player = game.Players.LocalPlayer

if gethwid then
    local hwid = gethwid()

    if hwid == "9c359a4c5d5ccd7d17d2d4adff3f113a956c0003a835aac3ab9ce2c9d064279e" then
        local function Callback(answer)
            if answer == "Yes" then 
                loadintrogui()
                wait(3)
                LoadKeySystem()
            elseif answer == "No" then
                LoadSySteMHuB()
            end 
        end

        local Bindable = Instance.new("BindableFunction")
        Bindable.OnInvoke = Callback

        game.StarterGui:SetCore("SendNotification", {
            Title = "Welcome Owner",
            Text = "Do You Want To Load The Key System",
            Duration = 300,
            Button1 = "Yes",
            Button2 = "No",
            Callback = Bindable
        })

    elseif hwid == "043cdacd574520c718bee341e1db7fac8e694704e0f13593f5631c325985f3b1" then
        game.StarterGui:SetCore("SendNotification", {
            Title = "رساله من احمد الطرش",
            Text = "منور يا انوس",
            Duration = 10,
        })
        LoadSySteMHuB()

    else
        loadintrogui()
        wait(3)
        LoadKeySystem()
    end

else
    loadintrogui()
    wait(3)
    LoadKeySystem()
end














